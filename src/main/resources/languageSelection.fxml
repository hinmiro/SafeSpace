<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10.0" xmlns:fx="http://javafx.com/fxml" fx:controller="controller.LanguageSelectionController">
    <padding>
        <Insets top="10" right="10" bottom="10" left="10"/>
    </padding>
    <Button text="English" onAction="#handleEnglish"/>
    <Button text="Finnish" onAction="#handleFinnish"/>
    <Button text="Japanese" onAction="#handleJapanese"/>
    <Button text="Russian" onAction="#handleRussian"/>
</VBox>